body
  background #ffffff
  margin 0
  mso-line-height-rule exactly
  mso-margin-bottom-alt 0px
  mso-margin-top-alt 0px
  mso-padding-alt 0px 0px 0px 0px
  padding 0

.preheader
  color #ffffff
  display none !important
  font-size 1px
  line-height 1px
  max-height 0px
  max-width 0px
  mso-hide all !important
  mso-line-height-rule exactly
  opacity 0
  overflow hidden
  visibility hidden

body > center
  table-layout fixed
  width 100%
  -webkit-font-smoothing antialiased
  -webkit-text-size-adjust 100%
  -ms-text-size-adjust 100%

table
  border-collapse collapse
  border-spacing 0
  margin 0
  mso-table-lspace 0
  mso-table-rspace 0
  padding 0

th
  font-size 0
  font-weight normal
  padding 0

img
  border 0
  display inline-block
  height auto
  max-width 100%

img.w100p
  border 0
  display block
  height auto
  width 100%

b
  font-weight bold

.txt
  // font-family Arial,sans-serif,Montserrat
  font-family -apple-system,BlinkMacSystemFont,"Segoe UI",Helvetica,Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol",Montserrat
  font-size 16 * 1px
  font-weight normal
  mso-line-height-rule exactly
  line-height (16 * 1.25) * 1px
  padding 0
  word-wrap break-word
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

for i in (8..96)
  if (i >= 20)
    .fz{i}
      font-size i * 1px
      mso-line-height-rule exactly
      line-height (i * 1.25) * 1px
  else
    .fz{i}
      font-size i * 1px
      mso-line-height-rule exactly
      line-height (i * 1.5) * 1px

n = 0
for i in (5 .. 200)
  if ((i % 10 == 0) || (i % 10 == 0.5))
    n = n + 1
    .p{n}
      padding 0
      padding-top i * 1px
      padding-right i * 1px
      padding-bottom i * 1px
      padding-left i * 1px
    .pt{n}
      padding-top i * 1px
    .pr{n}
      padding-right i * 1px
    .pb{n}
      padding-bottom i * 1px
    .pl{n}
      padding-left i * 1px

.p0
  padding 0
  padding-top 3px
  padding-right 3px
  padding-bottom 3px
  padding-left 3px
.pt0
  padding-top 3px
.pr0
  padding-right 3px
.pb0
  padding-bottom 3px
.pl0
  padding-left 3px
